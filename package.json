{
  "name": "server",
  "version": "1.0.0",
  "description": "MERN Stack Boilerplate Setup",
  "main": "server.js",
  "engines": {
    "node": "10.9.0",
    "npm": "6.4.1"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "webpack ./server.js --mode=development --watch",
    "prod": "webpack ./server.js --mode=production",
