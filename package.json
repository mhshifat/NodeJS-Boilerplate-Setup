    "express-fileupload": "^1.0.0",
    "helmet": "^3.14.0",
    "mongoose": "^5.3.6",
    "morgan": "^1.9.1"
  },
  "devDependencies": {
    "node-dev": "^3.1.3",
    "npm-run-all": "^4.1.3",
    "webpack": "^4.22.0",
    "webpack-cli": "^3.1.2",
    "webpack-node-externals": "^1.7.2"
  }
}
